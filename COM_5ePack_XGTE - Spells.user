    <tag group="sClass" tag="cHelpDrd"/>
    <tag group="sClass" tag="cHelpSor"/>
    <tag group="sClass" tag="cHelpWiz"/>
    </thing>
  <thing id="sp5CXGENeE" name="Negative Energy Flood" description="One creature you can see within range, unless the target is undead, must make a Constitution saving throw, taking 5d12 necrotic damage on a failed save, or half as much damage on a successful one. A target killed by this damage rises up as a zombie at the start of your next turn. The zombie pursues whatever creature it can see that is closest to it. Statistics for the zombie are in the Monster Manual. \n\nIf you target an undead with this spell, the target doesn’t make a saving throw. Instead, roll 5d12. The target gains half the total as temporary hit points." compset="Spell">
    <fieldval field="sCompDesc" value="a broken bone and a square of black silk"/>
    <fieldval field="sRange" value="60 feet"/>
    <fieldval field="sDuration" value="Instantaneous"/>
    <usesource source="5eXGTECP" parent="UserParent"/>
    <tag group="sLevel" tag="5"/>
