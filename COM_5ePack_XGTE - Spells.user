  <thing id="sp5CXGEAbE" name="Absorb Elements" description="Choose a damage type. You gain resistance to the given damage type until the start of your next turn. Also, the first time you hit with a melee attack on your next turn, the target takes an extra 1d6 damage of the given type, and the spell ends. \n\n{b}At Higher Levels.{/b}{/b} When you cast this spell using a spell slot of 2nd level or higher, the extra damage increases by 1d6 for each slot level above 1st." compset="Spell">
    <fieldval field="sRange" value="Self"/>
    <fieldval field="sDuration" value="1 round"/>
