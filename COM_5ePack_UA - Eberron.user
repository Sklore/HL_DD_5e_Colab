  <thing id="rarcshif" name="Razorclaw Shifting" description="On your turn, you can shift as a bonus action. Shifting lasts for 1 minute or until you end it on your turn as a bonus action.\nWhile shifting, you gain temporary hit points equal to your level + your Constitution bonus (minimum of 1). Also while shifting, you can make an unarmed strike as a bonus action. You can use your Dexterity for its attack roll and damage bonus, and this attack deals slashing damage.\nYou must finish a short or long rest before you can shift again." compset="RaceSpec" summary="You can shift into a more bestial version of yourself">
    <fieldval field="trkMax" value="1"/>
    <tag group="Usage" tag="ShortRest" name="/short rest" abbrev="/short rest"/>
    <tag group="User" tag="Activation" name="Activation" abbrev="Activation"/>
    <tag group="User" tag="Tracker" name="Tracker" abbrev="Tracker"/>
    <tag group="abAction" tag="Bonus" name="Bonus" abbrev="Bon"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="PostLevel" priority="9500" index="3"><![CDATA[      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)
      ~ If not active get out now!
      doneif (field[abilActive].value = 0)

      var searchexpr as string
      searchexpr = "IsWeapon.wUnarmed"

      foreach pick in hero from BaseWep where searchexpr
         perform eachpick.delete[DamageType.dtBludgeon]
         perform eachpick.assign[DamageType.dtSlashing]
         perform eachpick.assign[MelAttOpt.aDEX]
         perform eachpick.assign[DamageOpt.aDEX]
      nexteach]]>
      <before name="Default Unarmed Attack damage"/>
      </eval>
    </thing>
