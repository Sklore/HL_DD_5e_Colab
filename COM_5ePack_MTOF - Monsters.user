  <thing id="ra5CLegDetec" name="Detect" compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Legendary"/>
    <eval phase="Render" priority="100000"><![CDATA[      ~By default, we use the tRace herofield. If we want something else, we
      ~ set it in abText
      var name as string
      if (field[abText].isempty = 0) then
        name = field[abText].text
      else
        name = herofield[tRace].text
        endif

      field[CustDesc].text = "#name# can make a Wisdom (Perception) check."
      field[abSumm].text = "#name# can make a Wisdom (Perception) check."
      field[CustDesc].text = replace(field[CustDesc].text,"#name#",name,0)
      field[abSumm].text = replace(field[abSumm].text,"#name#",name,0)]]></eval>
    </thing>
