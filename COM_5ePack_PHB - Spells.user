  <thing id="cfg5CFndStd" name="Find Steed" compset="Configure" uniqueness="unique">
    <fieldval field="cfgLaMax1" value="1"/>
    <bootstrap thing="c5CStAlwAll"></bootstrap>
    <eval phase="Final"><![CDATA[

var searchexpr as string

searchexpr = master.tagids[SpeakLang.?," | "]
debug searchexpr

searchexpr = replace(searchexpr,"SpeakLang","thingid",0)
debug searchexpr
field[cfgLaAllo1].text = searchexpr]]></eval>
    <eval phase="First" index="2">hero.findchild[BaseRace].field[rINT].value = maximum(hero.findchild[BaseRace].field[rINT].value,-4)</eval>
    </thing>
  <thing id="c5CStAlwAll" name="Allow Any Creature" description="Select &quot;Yes&quot; to allow your steed to be any creature, not just those listed in the spell. You should get your DM&apos;s  permission before doing this." compset="CustomSpec" summary="Select &quot;Yes&quot; to allow your steed to be any creature." uniqueness="unique">
