    <tag group="sComp" tag="S"/>
    <tag group="sLevel" tag="1"/>
    <tag group="sCastTime" tag="Action1"/>
    <tag group="sDuration" tag="Instant"/>
    </thing>
  <thing id="sp5CAurLif" name="Aura of Life" description="Life-preserving energy radiates from you in an aura with a 30-foot radius. Until the spell ends, the aura moves with you, centered on you. Each nonhostile creature in the aura (including you) has resistance to necrotic damage, and its hit point maximum can&apos;t be reduced. In addition, a nonhostile, living creature regains 1 hit point when it starts its turn in the aura with 0 hit points." compset="Spell">
    <fieldval field="sArea" value="30-foot radius"/>
    <fieldval field="sDuration" value="up to 10 minutes"/>
    <fieldval field="sRange" value="Self"/>
    <usesource source="5ePHBCP"/>
