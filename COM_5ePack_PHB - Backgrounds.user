    <tag group="ProductId" tag="Wizards"/>
    <tag group="Background" tag="bck5CChar"/>
    </thing>
  <thing id="bt5CGambl" name="Gambler" description="I have a problem with gambling. Be it dice rolls or cards, I can&apos;t resist a potential win." compset="BackPerTrt" uniqueness="unique">
