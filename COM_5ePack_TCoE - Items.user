  <thing id="c5CTatCover" name="Tattoo Coverage" compset="CustomSpec" uniqueness="unique">
    <fieldval field="abValue" value="4"/>
    <fieldval field="abValue2" value="1"/>
    <fieldval field="abValue3" value="1"/>
    <eval phase="Final" priority="400"><![CDATA[var iLim as string
if (field[abValue].value > 0) then
iLim = field[abValue].value & " limb(s) not tattooed"
else
iLim = "All limbs tattooed"
endif

var iSca as string
if (field[abValue2].value > 0) then
iSca = "Scalp not tattooed"
else
iSca = "Scalp tattooed"
endif

var iTor as string
if (field[abValue3].value <= 0) then
iTor = "Torso tattooed"
elseif (field[abValue3].value = 0.5) then
iTor = "Half torso tattooed"
else
iTor = "Torso not tattooed"
endif

field[CustDesc].text = "{b}Limbs{/b}: " & iLim & "\n{b}Scalp{/b}: " & iSca & "\n{b}Torso{/b}: " & iTor]]></eval>
    </thing>
  <thing id="p5CRemTatLim" name="Remove Tattoo Limitations" description="This adjustment removes the limitations on tattoo coverage. You can put as many tatoos as you like on your character, disregarding their placement and coverage as outlined in Tasha&apos;s Cauldon of Everything." compset="InPlay" uniqueness="unique">
    <tag group="Helper" tag="NoIncr"/>
    <tag group="OthAdjCat" tag="Equipment"/>
    <eval phase="Final">doneif (field[pIsOn].value = 0)

hero.childfound[c5CTatCover].field[abValue].value += 999
hero.childfound[c5CTatCover].field[abValue2].value += 999
hero.childfound[c5CTatCover].field[abValue3].value += 999</eval>
    <eval phase="Final" priority="500" index="2"><![CDATA[doneif (field[pIsOn].value = 0)

hero.childfound[c5CTatCover].field[CustDesc].text = "You have added the adjustment to remove tattoo coverage limitations, and can add as many tattoos as you wish to your character."

perform hero.childfound[c5CTatCover].assign[Helper.SpecUp]]]></eval>
    </thing>
