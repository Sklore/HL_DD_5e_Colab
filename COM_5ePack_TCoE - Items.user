<?xml version="1.0" encoding="UTF-8"?>
<document signature="Hero Lab Data">
  <thing id="io5CAmDev1" name="Amulet of the Devout +1" description="While wearing this holy symbol, you gain a +1 bonus to spell attack rolls and the saving throw DCs of your spells.\n\nAlso while wearing it, you can use your Channel Divinity feature once without expending one of that feature&apos;s uses. This ability recharges the next dawn." compset="Wondrous">
    <fieldval field="gWeight" value="1"/>
    <fieldval field="trkMax" value="1"/>
    <fieldval field="abValue5" value="1"/>
    <fieldval field="miAttunTxt" value="requires attunement by a cleric or paladin"/>
    <usesource source="5eTCoE"/>
    <tag group="ItemRarity" tag="Uncommon"/>
    <tag group="User" tag="Tracker"/>
    <tag group="GearType" tag="gtWondrous"/>
    <tag group="Usage" tag="Day"/>
    <tag group="Helper" tag="EquipMag"/>
    <tag group="Helper" tag="NeedAttune"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Helper" tag="EquipAvail"/>
    <eval phase="PreLevel" priority="5000"><![CDATA[
    doneif (field[gIsEquip].value = 0)
    doneif (field[gIsAttuned].value = 0)

    var iLev as number
    var iPal as number
    var iClr as number

    iPal = #levelcount[Paladin]
    iClr = #levelcount[Cleric]
    iLev = iClr + iPal
    doneif (iLev < 1)

    if (iClr > 0) then
      hero.child[cHelpClr].field[cSplAttack].value = field[abValue5].value
      hero.child[cHelpClr].field[cSplSaveDC].value = field[abValue5].value
      endif

    if (iPal > 0) then
      hero.child[cHelpPal].field[cSplAttack].value = field[abValue5].value
      hero.child[cHelpPal].field[cSplSaveDC].value = field[abValue5].value
      endif]]></eval>
    </thing>
