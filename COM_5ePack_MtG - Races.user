  <thing id="ra5CSkiVe" name="Skill Versatility" description="Doesn&apos;t check for redundany proficiency and won&apos;t work for tools" compset="RaceSpec">
    <comment><![CDATA[Does not check if you're selecting a skill you're already proficient with.]]></comment>
    <fieldval field="usrCandid1" value="(component.BaseSkill | component.Tool) &amp; !Helper.Proficient"/>
    <fieldval field="usrCandid2" value="component.BaseSkill | component.Tools"/>
    <tag group="ChooseSrc2" tag="Thing"/>
    <tag group="Helper" tag="ActivMenu"/>
    <tag group="ChooseSrc1" tag="Thing"/>
    <tag group="Helper" tag="ShowSpec"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      doneif (tagis[Helper.ShowSpec] = 0)

      doneif (tagis[Helper.Disable] <> 0)

      var tagexpr as string

      if (field[usrChosen1].ischosen <> 0) then
        tagexpr = field[usrChosen1].chosen.tagids[ProfSkill.?," | "]
        ~debug tagexpr
        if (empty(tagexpr) <> 0) then
          ~proficient with tool code
        else
          foreach pick in hero from BaseSkill where tagexpr
            perform eachpick.assign[Helper.Proficient]
          nexteach
        endif
      endif

      if (field[usrChosen2].ischosen <> 0) then
        tagexpr = field[usrChosen2].chosen.tagids[ProfSkill.?," | "]
        ~debug tagexpr
        if (empty(tagexpr) <> 0) then
          ~proficient with tool code
        else
          foreach pick in hero from BaseSkill where tagexpr
            perform eachpick.assign[Helper.Proficient]
          nexteach
        endif
      endif]]></eval>
    </thing>
