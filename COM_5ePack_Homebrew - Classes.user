      doneif (tagis[Helper.Disable] <> 0)

      hero.childfound[Attack].field[atrBonus].value += field[abValue].value]]></eval>
    </thing>
  <thing id="cHB5CDueli" name="Dueling" description="When you are wielding a melee weapon in one hand and no other weapons, you gain a +2 bonus to damage rolls with that weapon." compset="CustomSpec" summary="+2 damage when wielding a melee weapon in one hand and no other weapons." uniqueness="unique">
    <fieldval field="abValue" value="2"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="abAction" tag="None" name="No action" abbrev="None"/>
    <tag group="abDuration" tag="Constant" name="Constant" abbrev="cons"/>
    <tag group="abRange" tag="Personal" name="Personal" abbrev="pers"/>
    <tag group="Helper" tag="Primary" name="Primary" abbrev="Primary"/>
    <tag group="abCategory" tag="BloHunFiSt"/>
    <tag group="SpecSource" tag="cHelpBlH"/>
