      elseif (field[pChosen].chosen.tagis[thingid.xTremorsen] <> 0) then
         field[pMaximum].value = 996
      elseif (field[pChosen].chosen.tagis[thingid.raTruesigh] <> 0) then
         field[pMaximum].value = 995
      endif]]></eval>
    </thing>
