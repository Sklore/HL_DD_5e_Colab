      doneif (tagis[Helper.ShowSpec] = 0)

      doneif (tagis[Helper.Disable] <> 0)

      perform root.linkage[table].assign[ArmProfGrp.ArmorMed]
      perform root.linkage[table].assign[ArmProfGrp.Shields]
      perform root.linkage[table].assign[ArmProfGrp.WepMartial]
      perform root.linkage[table].assign[ArmProfGrp.ArmorHeavy]]]></eval>
    </thing>
  <thing id="c5CMysPsRe" name="Psionic Resilience" description="At 3rd level, you learn to use psionic energy to speed up your natural healing. At the start of each of your turns, you gain temporary hit points equal to your Intelligence modifier, provided that you have at least 1 hit point." compset="ClSpecial" summary="Use psionic energy to speed up your natural healing.">
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="c5CMysSuHe" name="Surge of Health" description="Starting at 6th level, you can draw on your psychic focus to escape death’s grasp. As a reaction when you take damage, you can halve that damage against you. Your psychic focus immediately ends, and you can’t use it again until you finish a short or long rest. You can’t use this ability if you can’t use your psychic focus." compset="ClSpecial" summary="Spend your Psychic Focus to take half damage from an attack.">
    <tag group="abAction" tag="Reaction"/>
    </thing>
  <thing id="c5CMysCuRe" name="Cutting Resonance" description="At 8th level, you gain the ability to infuse your weapon attacks with psychic energy. Once on each of your turns when you hit a creature with a weapon, you can deal an extra 1d8 psychic damage to the target. When you reach 14th level, this extra damage increases to 2d8." compset="ClSpecial" summary="Once per round, deal extra radiant damage with weapon attack.">
    <fieldval field="abValue2" value="8"/>
    <fieldval field="trkMax" value="1"/>
    <tag group="LvNamePar" tag="DieCntVal" name="&quot;field[abValue].value&quot; dY" abbrev="&quot;field[abValue].value&quot; dY"/>
    <tag group="LvNamePar" tag="DieSizVal2" name="Xd &quot;field[abValue2].value&quot;" abbrev="Xd &quot;field[abValue2].value&quot;"/>
