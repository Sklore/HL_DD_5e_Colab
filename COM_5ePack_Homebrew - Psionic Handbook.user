  <thing id="c5CMysMaOr" name="Martial Order" description="At 1st level, you gain proficiency with martial weapons, heavy armor, and shields." compset="ClSpecial" summary="Gain proficiency with martial weapons, heavy armor, and shields.">
    <tag group="abAction" tag="None" name="No action" abbrev="None"/>
    <tag group="abDuration" tag="Constant" name="Constant" abbrev="cons"/>
