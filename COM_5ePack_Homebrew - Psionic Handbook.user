    <tag group="abDuration" tag="Round"/>
    <tag group="abAction" tag="Free"/>
    <tag group="abRange" tag="Attack"/>
    </thing>
  <thing id="c5CFtrImMB" name="Improved Mind and Body" description="Beginning at 18th level, when you use your action to cast a spell, you can make one weapon attack as a bonus action." compset="ClSpecial">
