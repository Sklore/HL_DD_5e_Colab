  <thing id="ra5CFeral" name="Feral" description="Your Intelligence score increases by 1, and your Dexterity score increases by 2. This trait replaces the Ability Score Increase trait." compset="VarRaceTrt" uniqueness="unique">
    <usesource source="5eSCAGCP"/>
    <tag group="AllowRCust" tag="r5CTieflin"/>
    <eval phase="First" priority="500"><![CDATA[~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      ~overwrite the tiefling's default 
      hero.child[r5CTieflin].field[rSTR].value = 0
      hero.child[r5CTieflin].field[rDEX].value = 2
      hero.child[r5CTieflin].field[rCON].value = 0
      hero.child[r5CTieflin].field[rINT].value = 1
      hero.child[r5CTieflin].field[rWIS].value = 0
      hero.child[r5CTieflin].field[rCHA].value = 0]]></eval>
    </thing>
  <thing id="ra5CDevTon" name="Devil&apos;s Tongue" description="You know the {i}vicious mockery{/i} cantrip. When you reach 3rd level, you can cast the {i}charm person{/i} spell as a 2nd-level spell once with this trait. When you reach 5th level , you can cast the {i}enthrall{/i} spell once with this trait. You must finish a long rest to cast these spells once again with this trait. Charisma is your spellcasting ability for them. This trait replaces the Infernal Legacy trait." compset="VarRaceTrt" uniqueness="unique">
