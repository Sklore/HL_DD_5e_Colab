  <thing id="ra5CElveKS" name="Keen Senses" description="You have proficiency in the Perception skill." compset="VarRaceTrt" uniqueness="unique">
    <usesource source="5eSCAGCP"/>
    <tag group="AllowRCust" tag="rHalfElf"/>
    <tag group="ProfSkill" tag="skPercep" name="Perception" abbrev="Perception"/>
    <tag group="RaReplace" tag="ra5CSkiVe"/>
    <eval phase="First" priority="11000"><![CDATA[
      ~if we've been disabled, get out now
      doneif (tagis[Helper.Disable] <> 0)

      ~we set these to zero to make the field disappear.
      hero.childfound[rHalfElf].field[cSkillMax].value=0
      perform hero.childfound[rHalfElf].delete[AllowSkl1.Any]

      ~assign our weapon proficiencies to the hero
      perform hero.pushtags[ProfSkill.?]]]></eval>
    </thing>
