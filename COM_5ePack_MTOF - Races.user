      <autotag group="Usage" tag="AtWill"/>
      <autotag group="Helper" tag="RaceSpell"/>
      </bootstrap>
    <eval phase="First" priority="500" index="3"><![CDATA[~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      ~overwrite the tiefling's default 
      hero.child[r5CTieflin].field[rSTR].value = 0
      hero.child[r5CTieflin].field[rDEX].value = 0
      hero.child[r5CTieflin].field[rCON].value = 0
      hero.child[r5CTieflin].field[rINT].value = 1
