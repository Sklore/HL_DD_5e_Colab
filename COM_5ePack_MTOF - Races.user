      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <bootstrap thing="spThaumatu">
      <containerreq phase="First" priority="2500">count:AbReplace.ra5CLegMal = 0</containerreq>
      <autotag group="Usage" tag="AtWill"/>
      <autotag group="Helper" tag="RaceSpell"/>
      </bootstrap>
    <eval phase="First" priority="500" index="3"><![CDATA[~ If we're disabled, do nothing
      doneif (tagis[Helper.Disable] <> 0)

      ~overwrite the tiefling's default 
      hero.child[r5CTieflin].field[rSTR].value = 0
      hero.child[r5CTieflin].field[rDEX].value = 0
      hero.child[r5CTieflin].field[rCON].value = 0
      hero.child[r5CTieflin].field[rINT].value = 1
      hero.child[r5CTieflin].field[rWIS].value = 0
      hero.child[r5CTieflin].field[rCHA].value = 2]]></eval>
    <eval phase="Final" priority="10000" index="2"><![CDATA[
      ~ If we're disabled, do nothing & 
      doneif (tagis[Helper.Disable] = 1)
      
      ~ We need to push the same tag to the hero as the
      ~ CORE/LW version of "Legacy of Maladomini".
      perform hero.assign[HasAbility.ra5CLegMal]]]></eval>
    <eval phase="First" priority="10"><![CDATA[
      ~ If we're disabled, do nothing & 
