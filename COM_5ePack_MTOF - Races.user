    <tag group="SubRace" tag="rGnome" name="Gnome" abbrev="Gnome"/>
    <tag group="Alignment" tag="NeutralGE" name="Neutral" abbrev="N"/>
    <tag group="Alignment" tag="NeutralLC" name="Neutral" abbrev="N"/>
