    </thing>
  <thing id="f5CToFSvM" name="Svirfneblin Magic" description="{i}Prerequisite: Gnome (deep gnome){/i}\n\nYou have inherited the innate spellcasting ability of your ancestors. This ability allows you to cast nondetection on yourself at will, without needing a material component. You can also cast each of the following spells once with this ability: blindness/deafness, blur, and disguise self. You regain the ability to cast these spells when you finish a long rest." compset="Feat" uniqueness="useronce">
    <comment><![CDATA[
    ~ If we're disabled, do nothing & 
    doneif (tagis[Helper.Disable] = 1)


    if (hero.child[spNondetec].field[sCL].value = 0) then
      hero.child[spNondetec].field[sCL].value = #totallevelcount[]
    endif
    if (hero.child[spBlinDeaf].field[sCL].value = 0) then
      hero.child[spBlinDeaf].field[sCL].value = #totallevelcount[]
    endif
    if (hero.child[spBlur].field[sCL].value = 0) then
      hero.child[spBlur].field[sCL].value = #totallevelcount[]
    endif
    if (hero.child[spDisgSelf].field[sCL].value = 0) then
      hero.child[spDisgSelf].field[sCL].value = #totallevelcount[]
    endif]]></comment>
    <usesource source="5eMTOFCP"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <bootstrap thing="spBlinDeaf">
      <autotag group="Helper" tag="RaceSpell"/>
      <autotag group="Usage" tag="Day"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <bootstrap thing="spBlur">
      <autotag group="Helper" tag="RaceSpell"/>
      <autotag group="Usage" tag="Day"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <bootstrap thing="spDisgSelf">
      <autotag group="Helper" tag="RaceSpell"/>
      <autotag group="Usage" tag="Day"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <bootstrap thing="spNondetec">
      <autotag group="Helper" tag="RaceSpell"/>
      <autotag group="Usage" tag="AtWill"/>
      </bootstrap>
    <pickreq thing="sr5CToFDGn"/>
    </thing>
