    <tag group="FeatureTyp" tag="Special" name="Special" abbrev="Special"/>
    <bootstrap thing="spShield">
      <containerreq phase="First" priority="2500"><![CDATA[(count:AbReplace.ra5CToFGzP = 0) & (count:Classes.? >= 3)]]></containerreq>
      <autotag group="Helper" tag="RaceSpell"/>
      <autotag group="Usage" tag="Day"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    <bootstrap thing="spMageHand">
      <containerreq phase="First" priority="2500">count:AbReplace.ra5CToFGzP = 0</containerreq>
      </bootstrap>
    <bootstrap thing="spDeteThou">
      <containerreq phase="First" priority="2500"><![CDATA[(count:AbReplace.ra5CToFGzP = 0) & (count:Classes.? >= 5)]]></containerreq>
      <autotag group="Helper" tag="RaceSpell"/>
      <autotag group="Usage" tag="Day"/>
      <assignval field="trkMax" value="1"/>
      </bootstrap>
    </thing>
  <thing id="ra5CToFGMD" name="Mental Discipline" description="\nYou have advantage on saving throws versus the charmed and frightened conditions." compset="RaceSpec">
    <usesource source="5eMTOFCP"/>
    <tag group="Helper" tag="ShowSpec" name="Show Spec" abbrev="Show Spec"/>
    <eval phase="Final" priority="10000"><![CDATA[
      ~if we've been replaced, get out now
      doneif (tagis[Helper.Disable] <> 0)
      #situational[hero.child[svAll],"Advantage vs. being charmed or frightened",field[thingname].text]]]></eval>
    </thing>
