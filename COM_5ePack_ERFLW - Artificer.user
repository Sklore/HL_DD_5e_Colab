    <usesource source="5eRftLWCP"/>
    <usesource source="5eTCoE"/>
    <tag group="RaceSize" tag="Tiny12" name="Tiny" abbrev="Tiny"/>
    <tag group="RaceType" tag="NPC" name="NPC Race" abbrev="NPC Race"/>
    <tag group="CondImm" tag="pcnExhaust"/>
