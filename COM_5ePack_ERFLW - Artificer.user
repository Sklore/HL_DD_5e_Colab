    <tag group="ChooseSrc1" tag="Hero" name="All Picks on Hero" abbrev="All Picks on Hero"/>
    <eval phase="PostLevel" priority="11000"><![CDATA[
     ~ If we're not enabled, get out now
     doneif (field[pIsOn].value = 0)
