    <tag group="AllowRCust" tag="cfg5CCFVRgr"/>
    <tag group="FeatureTyp" tag="Action"/>
    <tag group="abAction" tag="Action"/>
    <tag group="ChargeCalc" tag="AttrOnly"/>
    <tag group="ChargeCalc" tag="FinalMin1"/>
    <tag group="ChargeAttr" tag="aWIS"/>
    <tag group="Usage" tag="LongRest"/>
    <tag group="User" tag="Tracker"/>
    </thing>
  <thing id="cRgr5CSplFcs" name="Spellcasting Focus" description="You are able to use a druidic focus as the spellcasting focus for your ranger spells. The Player&apos;s Handbook lists some items that can be used as druidic foci in Chapter 5." compset="CustomSpec" uniqueness="unique">
