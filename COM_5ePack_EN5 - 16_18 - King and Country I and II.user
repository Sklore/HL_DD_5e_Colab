    <tag group="Background" tag="bck5CFeud"/>
    </thing>
  <thing id="bt5CAtonCr" name="Atone for Crime" description="I must atone for the crimes I committed in my family’s name." compset="BackBond">
    <usesource source="EN5_18"/>
