    <tag group="Background" tag="bck5CFeud"/>
    </thing>
  <thing id="bt5CVngenc" name="Vengeance" description="If you hurt my friends or family, your parents will wish you were never born. (Evil)" compset="BackIdeal">
    <usesource source="EN5_18"/>
    <tag group="Roll" tag="4"/>
    <tag group="Background" tag="bck5CFeud"/>
    </thing>
  <thing id="bt5CArrMrg" name="Arranged Marriage" description="I escaped an arranged marriage, so now two furious families and one determined betrothed hunt for me." compset="BackBond">
    <usesource source="EN5_18"/>
    <tag group="Roll" tag="1"/>
    <tag group="Background" tag="bck5CFeud"/>
    </thing>
  <thing id="bt5CChoice" name="Make Own Choices" description="I’ve lived my life serving others. Now I seek the freedom to make my own choices." compset="BackBond">
    <usesource source="EN5_18"/>
    <tag group="Roll" tag="2"/>
    <tag group="Background" tag="bck5CFeud"/>
    </thing>
  <thing id="bt5CRightP" name="Rightful Position" description="I will return my family to its rightful position of power, and punish those who forced our downfall." compset="BackBond">
    <usesource source="EN5_18"/>
