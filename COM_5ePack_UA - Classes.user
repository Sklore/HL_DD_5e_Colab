  <thing id="cfgUSFSCof" name="Chosen of the Gods" description="  At 1st level, you choose one of the cleric class’s divine domains. You add that domain’s spells for 1st-level clerics to your known spells. These spells do not count against the number of spells you can know, and they are considered to be sorcerer spells for you. When you reach 3rd, 5th, 7th, and 9th levels in the sorcerer class, you likewise learn your domain’s spells that become available at those levels." compset="Configure" uniqueness="unique">
    <fieldval field="cfgMax1" value="1"/>
    <fieldval field="cfgAllow1" value="component.Ability &amp; abCategory.ClrDomain"/>
    <fieldval field="cfgObject1" value="Favored Soul Domains"/>
    <tag group="ProductId" tag="5CUAFavSou"/>
    <tag group="Helper" tag="NoAdvLeag" name="Not Allowed for Adventurers League Characters" abbrev="Not Allowed for Adventurers League Characters"/>
    <tag group="StandardDC" tag="aCHA"/>
    <tag group="Helper" tag="ClsCastLev"/>
    <eval phase="First" priority="10000"><![CDATA[
      var searchexpr as string
      var domain as string
      searchexpr = "abCategory.ClrDomain & CustTaken." & idstring
      
      ~ Find the single cleric domain that was selected
      perform hero.findchild[Ability,searchexpr].setfocus
      doneif (state.isfocus = 0)

      searchexpr = ""
      ~ Loop through all the spells that the domain bootstraps and build
      ~ a search expression of them all.
      foreach bootstrap in focus where "component.BaseSpell"
        searchexpr = splice(searchexpr,eachthing.tagids[thingid.?],"|")
      nexteach

      ~ Make sure we find spells setup as Bonus Spells
      searchexpr = "(" & searchexpr & ") & BonusSplAt.?"
      domain = "thingid." & focus.idstring 

      ~ Loop through all the spells and make them sorcerer spells
      foreach pick in hero from BaseSpell where searchexpr
      
        ~ Make sure this spell is bootstrapped to the cleric domain
        if (eachpick.root.tagcountstr[domain] = 1) then
          perform eachpick.tagreplace[SpellType.cHelpClr,SpellType.cHelpSor]
          perform eachpick.tagreplace[Helper.Memorized,Helper.Known]     
        endif
      nexteach
      
      searchexpr = "SpecSource." & idstring

      ~ Loop through all the class specials and disable them from the domain
      foreach pick in hero from BaseClSpec where searchexpr
        perform eachpick.assign[Helper.Disable]
      nexteach]]></eval>
    <eval phase="PreLevel" index="3"><![CDATA[      
      ~ Set the configurable level &
      field[cfgLevel].value += hero.child[cHelpSor].field[cTotalLev].value]]></eval>
    </thing>
  <thing id="c5CFtrCQSh" name="Close Quarters Shooter" description="You are trained in making ranged attacks at close quarters. When making a ranged attack while you are within 5 feet of a hostile creature, you do not have disadvantage on the attack roll. Your ranged attacks ignore half cover and three-quarters cover against targets within 30 feet of you. Finally, you have a +1 bonus to attack rolls on ranged attacks." compset="CustomSpec" uniqueness="unique">
