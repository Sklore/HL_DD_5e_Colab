    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="Bonus"/>
    </thing>
  <thing id="ra5CBhlAvaBhl" name="Avatar of Bhaal" description="When the ravager drops to 0 hit points, its form shatters. Within the next 24 hours, it will reappear in its slayer form in a location within 2d6 miles of where its ravager form fell." compset="RaceSpec">
