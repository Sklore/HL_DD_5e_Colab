    <tag group="abSave" tag="aCON"/>
    <tag group="DamageType" tag="dtPiercing"/>
    </thing>
  <thing id="ra5CBrWCrsbEx" name="Crossbow Expert (Bonus)" description="Borivik can use a bonus action to add 1d10 to his next attack or damage roll with a crossbow." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="Bonus"/>
    </thing>
  <thing id="ra5CCnxCldAura" name="Cold Aura" description="Each time Cryonax&apos;s turn begins, all creatures within 10 feet of him suffer 21 (6d6) cold damage. Creatures that touch or hit him with a melee attack while within 10 feet of him also take 21 (6d6) cold damage. Nonmagical weapons that hit Cryonax are frozen immediately after dealing their damage. If the weapon is used again and hits any creature, it shatters. Otherwise the freezing effect disappears after an hour." compset="RaceSpec">
    <fieldval field="abRange" value="10"/>
    <fieldval field="wDieCount" value="6"/>
    <fieldval field="wDieSize" value="6"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    <tag group="DamageType" tag="dtCold"/>
    </thing>
  <thing id="ra5CCnxIceShrd" name="Ice Shroud" description="Cryonax gestures, targeting a frozen creature (see Tentacle Freeze) that is within 30 feet of him. The target is immediately encased in ice. The target takes 22 (5d8) cold damage immediately and at the beginning of each of their turns. The target is considered stunned, but the ice provides resistance to all damage. The shroud disappears if Cryonax removes it, Cryonax dies, the shroud takes 100 hit points of damage or another character uses their action to make a DC 22 Strength check to remove the trapped ally." compset="RaceSpec">
    <fieldval field="abRange" value="30"/>
    <fieldval field="wDieCount" value="5"/>
    <fieldval field="wDieSize" value="8"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="abRange" tag="Feet"/>
    <tag group="FeatureTyp" tag="Legendary"/>
    <tag group="abAction" tag="Action"/>
    <tag group="DamageType" tag="dtCold"/>
    <tag group="LegenAct" tag="3"/>
    </thing>
  <thing id="ra5CCnxIceWlk" name="Ice Walker" description="Cryonax doesn&apos;t need to make ability checks when walking or climbing across icy surfaces. Moreover, deep snow and icy terrain do not cost him extra movement." compset="RaceSpec">
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="FeatureTyp" tag="Special"/>
    <tag group="abAction" tag="None"/>
    </thing>
  <thing id="ra5CCnxSmash" name="Smash" description="Cryonax slams one grappled target into the ground, dealing 18 (4d8) bludgeoning damage to it." compset="RaceSpec">
