  <thing id="c5CWizTrSt" name="Transmuterâ€™s Stone" description="Placeholder..." compset="ClSpecial" summary="Placeholder...">
    <fieldval field="actName" value="In Own Possession"/>
    <arrayval field="usrArray" index="0" value="Darkvision"/>
    <arrayval field="usrArray" index="1" value="Increase speed"/>
    <arrayval field="usrArray" index="2" value="Proficiency with Con saves"/>
    <arrayval field="usrArray" index="3" value="Resist acid"/>
    <arrayval field="usrArray" index="4" value="Resist cold"/>
    <arrayval field="usrArray" index="5" value="Resist fire"/>
    <arrayval field="usrArray" index="6" value="Resist lightning"/>
    <arrayval field="usrArray" index="7" value="Resist thunder"/>
    <tag group="abDuration" tag="Permanent"/>
    <tag group="abRange" tag="Touch"/>
    <tag group="User" tag="Activation"/>
    <tag group="abAction" tag="Hour"/>
    <tag group="Helper" tag="ActivMenu"/>
    <bootstrap thing="raDarkVis">
      <containerreq phase="First" priority="500"><![CDATA[fieldval:usrIndex = 0 & fieldval:abilActive <> 0]]></containerreq>
      <assignval field="abValue" value="60"/>
      </bootstrap>
    <eval phase="PostLevel" priority="10000"><![CDATA[field[abValue].value += #attrmod[aCHA]

    doneif (tagis[Helper.ShowSpec] = 0)

    doneif (tagis[Helper.Disable] <> 0)

    doneif (field[abilActive].value = 0)

    ~ Darkvision
    if (field[usrIndex].value = 0) then
      ~handled by a bootstrap condition

    ~ Speed
    elseif (field[usrIndex].value = 1) then
      hero.child[Speed].field[tSpeed].value += 10

    ~ Con proficiency
    elseif (field[usrIndex].value = 2) then
      perform hero.assign[ProfSave.svCON]

    ~ Resisitance (acid)
    elseif (field[usrIndex].value = 3) then
      perform hero.assign[DamageRes.dtAcid]

    ~ Resisitance (cold)
    elseif (field[usrIndex].value = 4) then
      perform hero.assign[DamageRes.dtCold]

    ~ Resisitance (fire)
    elseif (field[usrIndex].value = 5) then
      perform hero.assign[DamageRes.dtFire]

    ~ Resisitance (lightning)
    elseif (field[usrIndex].value = 6) then
      perform hero.assign[DamageRes.dtLightnin]

    ~ Resisitance (thunder)
    elseif (field[usrIndex].value = 7) then
      perform hero.assign[DamageRes.dtThunder]
      endif]]></eval>
    </thing>
