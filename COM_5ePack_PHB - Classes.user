  <thing id="c5CWlkGrOO" name="The Great Old One" description="A mysterious entity from beynd reality is your patron. It&apos;s motives are incomprehensible, but it&apos;s knowledge vast." compset="CustomSpec" uniqueness="unique">
    <tag group="SpecSource" tag="cHelpWlk"/>
    <tag group="abCategory" tag="WlkOthPatr" name="Warlock Otherworldly Patron"/>
    <tag group="Helper" tag="Primary"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="ClsAllowSp" tag="sp5CDissW"/>
    <tag group="ClsAllowSp" tag="spHideLaug"/>
    <tag group="ClsAllowSp" tag="spDeteThou"/>
    <tag group="ClsAllowSp" tag="sp5CPhantF"/>
    <tag group="ClsAllowSp" tag="spClairvoy"/>
    <tag group="ClsAllowSp" tag="spSending"/>
    <tag group="ClsAllowSp" tag="spDomiBeas"/>
    <tag group="ClsAllowSp" tag="spBlacTent"/>
    <tag group="ClsAllowSp" tag="spDomiPers"/>
    <tag group="ClsAllowSp" tag="spTelekine"/>
